{
  "name": "autonomous-log-simulator",
  "version": "1.0.0",
  "description": "Autonomous log generation app for testing ETL pipelines",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node src/app.ts",
    "watch": "nodemon --watch src --ext ts --exec ts-node src/app.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "logs",
    "simulator",
    "etl",
    "winston",
    "logging",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "express": "^4.18.2",
    "ws": "^8.16.0",
    "kafkajs": "^2.2.4",
    "ioredis": "^5.3.2",
    "better-sqlite3": "^9.2.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "@types/express": "^4.17.21",
    "@types/ws": "^8.5.10",
    "@types/cors": "^2.8.17",
    "@types/better-sqlite3": "^7.6.8",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
